{
	"manifest_version": 2,
	"name" :"Atexo Autoplaf",
	"version" :"1.0",
	"description" :"Plateforme automatique",
	"browser_action": {
      "default_title": "Atexo Autoplaf",
		"default_icon": "images/icon.png",
		"default_popup" :"atx.html"
	},
	"icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Ctrl+Shift+A",
            "default": "Ctrl+Shift+A",
            "linux": "Ctrl+Shift+A",
            "mac": "Command+Shift+A",
            "windows": "Ctrl+Shift+A"
         }
      }
   }
}